{
	"name": "instant-miso-kitchen",
	"version": "1.0.1",
	"description": "MISO | Minimal Initial SushiSwap Offering",
	"author": "MISO",
	"private": true,
	"main": "index.js",
	"scripts": {
		"generate": "cross-env NODE_ENV=production nuxt generate",
		"generate-icons": "vsvg -s ./app/assets/svg -t ./app/assets/icons",
		"lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
		"lint": "yarn lint:js",
		"dev": "cross-env NODE_ENV=development nuxt ",
		"build": "NODE_ENV=production nuxt build",
		"postbuild": "webpack",
		"start": "NODE_ENV=production node ./.nuxt/dist/server.js",
		"generate:error": "cross-env NODE_ENV=production GENERATE_ERROR_PAGE=true nuxt generate",
		"precommit": "npm run lint",
		"sls": "sls",
		"sls:deploy": "sls deploy",
		"cf:deploy": "cross-env NODE_ENV=production ENDPOINT_ENV=cf node bin/deploy/index.js",
		"deploy:dev": "sls deploy --stage dev",
		"deploy:stage": "NODE_ENV=production sls deploy --stage stage",
		"deploy:prod": "NODE_ENV=production sls deploy --stage prod",
		"undeploy:dev": "sls remove --stage dev",
		"undeploy:stage": "NODE_ENV=production sls remove --stage stage",
		"undeploy:prod": "NODE_ENV=production sls remove --stage prod"
	},
